{% block body_javascripts %}
  <script>
    var observer = new IntersectionObserver(function (entries) {
        entries.forEach((entry) => {
          let id = entry.target.id;
          let buttonId = id.replace('img', 'menu');
          let menu = document.getElementById(buttonId);

          if (entry.isIntersecting === true) {
            menu.firstElementChild.children[0].classList.add('d-none');
            menu.firstElementChild.children[1].classList.remove('d-none');
          } else {
            menu.firstElementChild.children[0].classList.remove('d-none');
            menu.firstElementChild.children[1].classList.add('d-none');
          }
        });
      },
      // full visible
      { threshold: [1] }
    );

    Array.from(document.querySelectorAll(".img-icon-frame")).forEach((item) => {
      observer.observe(item);
    });

    document.onscroll = function(event) {
      if (!window.scrollTimeout) {
        document.getElementById('fixed-menu').style.display = 'block';
      }

      clearTimeout(window.scrollTimeout);
      window.scrollTimeout = setTimeout(function() {
        document.getElementById('fixed-menu').style.display = 'none';
        window.scrollTimeout = null;
      }, 3000);
    }
  </script>
  <script>
    $('#intro').on('hidden.bs.modal', function (event) {
      $('#intro iframe').attr('src', $('#intro iframe').attr('src'));
    })
  </script>
{% endblock %}